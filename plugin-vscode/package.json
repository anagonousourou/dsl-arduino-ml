{
    "name": "arduinoml-vscode-plugin",
    "displayName": "ArduinoML",
    "description": "Language support for ArduinoML in vscode",
    "author": "ANAGONOU Patrick",
    "version": "1.0.0",
    "keywords": [
      "arduino",
      "language",
      "support",
      "syntax",
      "highlighting"
    ],
    "categories": [
        "Programming Languages"
    ],
    "license": "UNLICENSED",
    "engines": {
        "vscode": "^1.50.0"
    },
    "contributes": {
        "languages": [{
            "id": "arduinoml",
            "aliases": ["ArduinoML", "arduinoml"],
            "extensions": [".arduinoml"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "arduinoml",
            "scopeName": "source.arduinoml",
            "path": "./syntaxes/arduinoml.tmLanguage.json"
        }]
    }
}
