application redButton

# Declaring bricks
sensor button: 9
actuator buz: 12
actuator ll: 11

# Declaring states
stateA {
    buz <= HIGH
    button is HIGH => stateB
}

stateB {
    buz <= LOW
    ll <= HIGH
    button is HIGH => start
}

-> start {
    buz <= LOW
    ll <= LOW
    button is HIGH => stateA 
}
